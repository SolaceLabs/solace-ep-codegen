		"acmeretail/onlineservices/customer/created/v2/{regionId}/{customerId}": {
			"publish": {
				"bindings": {
					"solace": {
						"destinations": [
							{
								"destinationType": "queue",
								"queue": {
									"name": "SHIPPING.CUSTOMERS",
									"topicSubscriptions": [
										"acmeretail/onlineservices/customer/*/v2/*/*"
									]
